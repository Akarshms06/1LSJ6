import java.io.*;
class GFG {
    static double Series(int n) {
        int i;
        double sums = 0.0;
        for (i = 1; i <= n; i++)
            sums += 1.0 / (i * i);
        return sums;
    }
    public static void main(String[] args) {
        int n = 3;
        double res = Series(n);
        System.out.println(res);
    }
}